# This tmux statusbar config was created by tmuxline.vim
# on Wed, 04 Sep 2019

set -g status-justify "left"
set -g status "on"
set -g status-left-style "none"
set -g message-command-style "fg=#eeffff,bg=#424242"
set -g status-right-style "none"
set -g pane-active-border-style "fg=#89ddff"
set -g status-style "none,bg=#2c2c2c"
set -g message-style "fg=#eeffff,bg=#424242"
set -g pane-border-style "fg=#424242"
set -g status-right-length "100"
set -g status-left-length "100"
setw -g window-status-activity-style "none,fg=#89ddff,bg=#2c2c2c"
setw -g window-status-separator ""
setw -g window-status-style "none,fg=#65737e,bg=#2c2c2c"
set -g status-left "#[fg=#212121,bg=#89ddff] [#S] #[fg=#89ddff,bg=#2c2c2c,nobold,nounderscore,noitalics]"
set -g status-right "#[fg=#89ddff,bg=#2c2c2c,nobold,nounderscore,noitalics]#[fg=#212121,bg=#89ddff] \"#22T\" %H:%M %d-%b-%y "
setw -g window-status-format "#[fg=#2c2c2c,bg=#2c2c2c,nobold,nounderscore,noitalics]#[default] #I:#W#F #[fg=#2c2c2c,bg=#2c2c2c,nobold,nounderscore,noitalics]"
setw -g window-status-current-format "#[fg=#2c2c2c,bg=#424242,nobold,nounderscore,noitalics]#[fg=#eeffff,bg=#424242] #I:#W#F #[fg=#424242,bg=#2c2c2c,nobold,nounderscore,noitalics]"
