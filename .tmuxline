# This tmux statusbar config was created by tmuxline.vim
# on Sun, 28 Jul 2019

set -g status-justify "left"
set -g status "on"
set -g status-left-style "none"
set -g message-command-style "fg=#1E1E3F,bg=#A599E9"
set -g status-right-style "none"
set -g pane-active-border-style "fg=#1E1E3F"
set -g status-style "none,bg=#1E1E3F"
set -g message-style "fg=#1E1E3F,bg=#A599E9"
set -g pane-border-style "fg=#A599E9"
set -g status-right-length "100"
set -g status-left-length "100"
setw -g window-status-activity-style "none"
setw -g window-status-separator ""
setw -g window-status-style "none,fg=#A599E9,bg=#1E1E3F"
set -g status-left "#[fg=#A599E9,bg=#1E1E3F] #S #[fg=#1E1E3F,bg=#1E1E3F,nobold,nounderscore,noitalics]"
set -g status-right "#[fg=#A599E9,bg=#1E1E3F,nobold,nounderscore,noitalics]#[fg=#1F1F41,bg=#A599E9] %Y-%m-%d | %H:%M #[fg=#1E1E3F,bg=#A599E9,nobold,nounderscore,noitalics]#[fg=#A599E9,bg=#1E1E3F] #h "
setw -g window-status-format "#[fg=#A599E9,bg=#1E1E3F] #I |#[fg=#A599E9,bg=#1E1E3F] #W "
setw -g window-status-current-format "#[fg=#1E1E3F,bg=#A599E9,nobold,nounderscore,noitalics]#[fg=#1E1E3F,bg=#A599E9] #I |#[fg=#1E1E3F,bg=#A599E9] #W #[fg=#A599E9,bg=#1E1E3F,nobold,nounderscore,noitalics]"
