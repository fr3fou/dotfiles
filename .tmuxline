# This tmux statusbar config was created by tmuxline.vim
# on Fri, 13 Mar 2020

set -g status-justify "left"
set -g status "on"
set -g status-left-style "none"
set -g message-command-style "fg=#d6deeb,bg=#112630"
set -g status-right-style "none"
set -g pane-active-border-style "fg=#82aaff"
set -g status-style "none,bg=#112630"
set -g message-style "fg=#d6deeb,bg=#112630"
set -g pane-border-style "fg=#112630"
set -g status-right-length "100"
set -g status-left-length "100"
setw -g window-status-activity-style "none"
setw -g window-status-separator ""
setw -g window-status-style "none,fg=#d6deeb,bg=#112630"
set -g status-left ""
set -g status-right "#[fg=#d6deeb,bg=#112630] %H:%M "
setw -g window-status-format "#[fg=#d6deeb,bg=#112630] #I #[fg=#d6deeb,bg=#112630]#W "
setw -g window-status-current-format "#[fg=#112630,bg=#112630,nobold,nounderscore,noitalics]#[fg=#d6deeb,bg=#112630] #W #[fg=#112630,bg=#112630,nobold,nounderscore,noitalics]"
